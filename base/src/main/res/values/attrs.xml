<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CEditText">
        <attr name="height" format="dimension" />
        <attr name="count" format="integer" />
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineColor" format="color"/>
        <attr name="focusLineColor" format="color" />
        <attr name="stokesColor" format="color"/>
        <attr name="focusStokeColor" format="color" />
        <attr name="spaceWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="radius" format="dimension" />
        <attr name="circleColor" format="color" />
        <attr name="isDrawCircle" format="boolean" />
        <attr name="isDrawLine" format="boolean"/>
    </declare-styleable>



    <declare-styleable name="CSEditText">
        <attr name="isClose" format="boolean"/>
        <attr name="isPass" format="boolean"/>
        <attr name="hintStr" format="string"/>
        <attr name="android:inputType"/>
        <attr name="maxLength" format="integer"/>

    </declare-styleable>


</resources>